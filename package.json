{
  "name": "react-calendar-app",
  "description": "single-page-application that allows us to schedule events",
  "version": "0.0.1",
  "author": "Alexey Pedyashev",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "es6",
    "calendar"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/apedyashev/react-calendar-app.git"
  },
  "bugs": "https://github.com/apedyashev/react-calendar-app/issues",
  "scripts": {
    "precommit": "npm test",
    "pretest": "eslint ./src --ext .js",
    "clean:build": "node ./bin/clean.js createdir",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "node ./bin/clean.js",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "lint": "eslint ./src --ext .js",
    "jscs": "jscs src/js/",
    "test": "env NODE_PATH=src ava",
    "test:watch": "npm run test -- --watch",
    "deploy": "./bin/gh-pages-deploy.sh"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-webpack-loaders": "0.8.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "css-loader": "0.25.0",
    "css-modules-require-hook": "4.0.1",
    "enzyme": "2.5.1",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.5.0",
    "file-loader": "0.9.0",
    "husky": "0.11.9",
    "jsdom": "9.8.3",
    "postcss-loader": "1.1.0",
    "react-addons-test-utils": "15.2.0",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.1.1",
    "redux-immutable-state-invariant": "1.2.4",
    "rimraf": "2.5.4",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack-dev-server": "1.16.2",
    "webpack-strip": "0.1.0"
  },
  "dependencies": {
    "autoprefixer": "6.5.1",
    "bootstrap": "3.3.7",
    "classnames": "2.2.5",
    "copy-webpack-plugin": "4.0.0",
    "eslint-plugin-import": "2.1.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fullcalendar": "3.1.0",
    "history": "3.2.1",
    "html5-notification": "3.0.0",
    "humps": "2.0.0",
    "jquery": "3.1.1",
    "lodash": "4.16.6",
    "lost": "7.1.0",
    "moment": "2.17.1",
    "normalizr": "2.2.1",
    "postcss-nested": "1.0.0",
    "postcss-simple-vars": "3.0.0",
    "qs": "6.3.0",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-big-calendar": "0.11.1",
    "react-bootstrap": "0.30.7",
    "react-datepicker": "0.39.0",
    "react-dom": "15.3.2",
    "react-helmet": "3.2.3",
    "react-icons": "2.2.1",
    "react-overlays": "0.6.10",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.6.0",
    "redux-form": "6.3.2",
    "redux-logger": "2.7.4",
    "redux-saga": "0.12.1",
    "twix": "1.1.3",
    "webpack": "1.14.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register",
      "css-modules-require-hook/preset",
      "./src/helpers/test.js"
    ],
    "babel": "inherit"
  }
}
